<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pasantias.redriapp">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activities.SuperficieLimaActivity"></activity>
        <activity android:name=".activities.IminaLimaActivity" />
        <activity android:name=".activities.OTLimaActivity" />
        <activity android:name=".activities.DuracionActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.OrdenActivity" />
        <activity android:name=".activities.RegisterActivity">
            <meta-data
                android:name="DATABASE"
                android:value="sugarormapp.db" />
            <meta-data
                android:name="VERSION"
                android:value="8" />
            <meta-data
                android:name="QUERY_LOG"
                android:value="true" />
            <meta-data
                android:name="DOMAIN_PACKAGE_NAME"
                android:value="com.pasantias.redriapp.models" />
        </activity>
        <activity android:name=".activities.MainActivity" />
    </application>

</manifest>